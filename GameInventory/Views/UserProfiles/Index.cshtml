@using GameInventory.Models;
@{
  Layout = "_Layout";
}

<h1 class="jumbotron">UserProfiles</h1>

@if (@Model.Count == 0)
{
  <h3>No UserProfiles here!</h3>
} 

@foreach (UserProfile userProfile in Model)
{
  <h3><a href='/userProfiles/@userProfile.Id'>@userProfile.Name</a></h3>
}

<p><a href='/'>Back home</a></p>